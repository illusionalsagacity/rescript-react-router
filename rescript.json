{
  "name": "rescript-react-router",
  "bsc-flags": ["-open RescriptCore"],
  "jsx": {
    "version": 4
  },
  "bs-dev-dependencies": [],
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react",
    "@glennsl/rescript-fetch"
  ],
  "sources": [
    {
      "dir": "src",
      "subdirs": true,
      "public": ["ReactRouter"]
    },
    {
      "dir": "__tests__",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "suffix": ".res.mjs",
    "module": "esmodule",
    "in-source": true
  }
}
